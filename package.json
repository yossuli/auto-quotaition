{
  "name": "auto-quotation",
  "displayName": "auto-quotation",
  "description": "",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.84.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:auto-quotation.auto-quotation",
    "onCommand:auto-quotation.auto-dis-quotation",
    "onCommand:auto-quotation.auto-quotationWithSpace"
  ],
  "main": "./dist/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "auto-quotation.contextmenu.auto-quotation",
        "title": "カンマ区切りでそれぞれクォーテーションで囲む"
      },
      {
        "command": "auto-quotation.contextmenu.auto-quotationWithSpace",
        "title": "空白区切りでそれぞれクォーテーションで囲んでカンマをつける"
      },
      {
        "command": "auto-quotation.contextmenu.auto-dis-quotation",
        "title": "それぞれのクォーテーションを外す"
      }
    ],
    "menus": {
      "editor/context": [
        {
          "when": "editorHasSelection",
          "command": "auto-quotation.contextmenu.auto-quotation",
          "group": "myGroup@1"
        },
        {
          "when": "editorHasSelection",
          "command": "auto-quotation.contextmenu.auto-quotationWithSpace",
          "group": "myGroup@2"
        },
        {
          "when": "editorHasSelection",
          "command": "auto-quotation.contextmenu.auto-dis-quotation",
          "group": "myGroup@3"
        }
      ]
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run package",
    "compile": "webpack",
    "watch": "webpack --watch",
    "package": "webpack --mode production --devtool hidden-source-map",
    "compile-tests": "tsc -p . --outDir out",
    "watch-tests": "tsc -p . -w --outDir out",
    "pretest": "npm run compile-tests && npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/vscode": "^1.84.0",
    "@types/mocha": "^10.0.3",
    "@types/node": "18.x",
    "@typescript-eslint/eslint-plugin": "^6.9.0",
    "@typescript-eslint/parser": "^6.9.0",
    "eslint": "^8.52.0",
    "glob": "^10.3.10",
    "mocha": "^10.2.0",
    "typescript": "^5.2.2",
    "ts-loader": "^9.5.0",
    "webpack": "^5.89.0",
    "webpack-cli": "^5.1.4",
    "@vscode/test-electron": "^2.3.6"
  }
}
